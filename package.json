{
  "name": "vite-admin",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "description": "vite-admin open source management system",
  "author": {
    "name": "Yogi",
    "email": "18435179812@163.com",
    "url": "https://github.com/codebabydxj"
  },
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "serve": "vite --open",
    "build": "node --max_old_space_size=1024000 ./node_modules/vite/bin/vite.js build",
    "build:dev": "vite build --mode development",
    "build:test": "vite build --mode test",
    "build:pro": "vite build --mode production",
    "preinstall": "npx only-allow pnpm",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "preview": "pnpm build:dev && vite preview",
    "lint": "vite lint",
    "lint:eslint": "eslint --fix --ext .js,.ts,.vue ./src",
    "lint:prettier": "prettier --write \"src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "clean": "rmdir /s /q node_modules",
    "reinstall": "pnpm clean && pnpm install",
    "commit": "git add -A && czg && git push"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@vicons/antd": "^0.13.0",
    "@vueuse/core": "^13.6.0",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "axios": "^1.11.0",
    "axios-jsonp": "^1.0.4",
    "codemirror": "5.65.19",
    "codemirror-editor-vue3": "^2.8.0",
    "dayjs": "^1.11.13",
    "driver.js": "^1.3.1",
    "echarts": "^5.6.0",
    "echarts-liquidfill": "^3.1.0",
    "element-plus": "^2.10.6",
    "js-md5": "^0.8.3",
    "lodash": "^4.17.21",
    "mitt": "^3.0.1",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.4.1",
    "qs": "^6.13.1",
    "screenfull": "^6.0.2",
    "sortablejs": "^1.15.6",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "vue3-puzzle-vcode": "^1.1.7",
    "vuedraggable": "^4.1.0",
    "xgplayer": "^3.0.22"
  },
  "devDependencies": {
    "@types/codemirror": "^5.60.15",
    "@types/js-md5": "^0.7.2",
    "@types/lodash": "^4.17.14",
    "@types/node": "^20.17.12",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.17",
    "@types/sortablejs": "^1.15.8",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "czg": "^1.11.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.32.0",
    "prettier": "^3.4.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "sass": "^1.83.1",
    "terser": "^5.37.0",
    "typescript": "^5.7.3",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-setup-extend-plus": "^1.0.1",
    "vite": "^5.4.11",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^2.2.0"
  },
  "engines": {
    "node": "^18.18.0 || >=20.9.0",
    "pnpm": ">=9"
  },
  "browserslist": {
    "production": [
      "> 1%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
