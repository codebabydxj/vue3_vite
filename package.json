{
  "name": "vite-app",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "serve": "vite --open",
    "build:dev": "vite build --mode development",
    "build:pro": "vite build --mode production",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "preview": "vite preview",
    "lint": "vite lint",
    "format": "prettier --write \"./**/*.{html,vue,ts,js,json,md}\"",
