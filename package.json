    "axios": "^1.2.0",
    "axios-jsonp": "^1.0.4",
    "axios-retry": "^3.3.1",
    "dayjs": "^1.11.6",
    "driver.js": "^0.9.8",
    "echarts": "^5.4.1",
    "echarts-liquidfill": "^3.1.0",
    "element-plus": "^2.4.2",
    "js-md5": "^0.7.3",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "normalize.css": "^8.0.1",
