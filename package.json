{
  "name": "vite-admin",
  "private": true,
  "version": "1.0.1",
  "description": "vite-admin open source management system",
  "author": {
    "name": "Yogi",
    "email": "18435179812@163.com",
    "url": "https://github.com/codebabydxj"
  },
  "scripts": {
    "dev": "vite",
    "serve": "vite --open",
    "build": "node --max_old_space_size=1024000 ./node_modules/vite/bin/vite.js build",
    "build:dev": "vite build --mode development",
    "build:test": "vite build --mode test",
    "build:pro": "vite build --mode production",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "preview": "pnpm build:dev && vite preview",
    "lint": "vite lint",
    "format": "prettier --write \"./**/*.{html,vue,ts,js,json,md}\"",
    "clean": "rmdir /s /q node_modules",
    "reinstall": "pnpm clean && pnpm install",
    "commit": "git add -A && czg && git push"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@vicons/antd": "^0.12.0",
    "@vueuse/core": "^10.11.0",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "axios": "^1.7.2",
    "axios-jsonp": "^1.0.4",
    "dayjs": "^1.11.11",
    "driver.js": "^1.3.1",
    "echarts": "^5.5.1",
    "echarts-liquidfill": "^3.1.0",
    "element-plus": "^2.9.3",
    "js-md5": "^0.8.3",
    "lodash": "^4.17.21",
    "mitt": "^3.0.1",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "qs": "^6.12.1",
    "screenfull": "^6.0.2",
    "sortablejs": "^1.15.2",
    "vue": "^3.4.35",
    "vue-router": "^4.4.5",
    "vue3-puzzle-vcode": "^1.1.7",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@types/js-md5": "^0.7.2",
    "@types/lodash": "^4.17.6",
    "@types/node": "^20.14.9",
    "@types/nprogress": "^0.2.3",
    "@types/qs": "^6.9.15",
    "@types/sortablejs": "^1.15.8",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "czg": "^1.9.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.77.6",
    "terser": "^5.31.1",
    "typescript": "^5.5.2",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-setup-extend-plus": "^1.0.1",
    "vite": "^4.5.3",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^2.0.24"
  },
  "engines": {
    "node": ">=16.18.0"
  },
  "browserslist": {
    "production": [
      "> 1%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
